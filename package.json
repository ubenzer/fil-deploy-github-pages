{
  "name": "fil-deploy-github-pages",
  "version": "1.0.0",
  "description": "A helper tool for fil to deploy projects to github pages easily",
  "scripts": {
    "compile": "shx --silent rm -rf compiled && babel app -d compiled",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix"
  },
  "bin": {
    "fil-deploy-sync": "./compiled/bin/sync.js",
    "fil-deploy-clean": "./compiled/bin/clean.js",
    "fil-deploy-deploy": "./compiled/bin/deploy.js"
  },
  "author": "Umut Benzer <umut@benzer.me>",
  "license": "MIT",
  "dependencies": {
    "debug": "^2.6.8",
    "fs-extra": "^3.0.1",
    "globby": "^6.1.0",
    "moment": "^2.18.1"
  },
  "devDependencies": {
    "babel-cli": "^6.24.1",
    "babel-core": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "eslint": "^3.19.0",
    "eslint-plugin-react": "^7.0.1",
    "shx": "^0.2.2"
  },
  "repository": {
    "type": "git",
    "url": "git@github.com:ubenzer/fil-deploy-github-pages.git"
  },
  "bugs": {
    "url": "https://github.com/ubenzer/fil-deploy-github-pages/issues"
  }
